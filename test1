SELECT
  a.*,
  -- 右表关键字段全部带上前缀，确保列名不重复
  e.PRODUCT_ID       AS EXP_PRODUCT_ID,
  e.ALT_ID           AS EXP_ALT_ID,
  e.CUSTOMER         AS EXP_CUSTOMER,
  e.PAYMENT_METHOD   AS EXP_PAYMENT_METHOD,
  e.PAYMENT_FREQ     AS EXP_PAYMENT_FREQ,
  e.PERIOD_START     AS EXP_PERIOD_START,
  e.PERIOD_END       AS EXP_PERIOD_END,
  e.DENOM            AS EXP_DENOM,
  e.PRINCIPAL        AS EXP_PRINCIPAL,
  e.RATE_DECIMAL     AS EXP_RATE_DECIMAL,
  e.DAILY_ACCRUAL    AS EXP_DAILY_ACCRUAL,
  e.EXPECTED_INTEREST,
  e.NEXT_PRINCIPAL_IF_CAPITALISE
FROM DB_INVESTMENTS_SYST02.INVESTMENTS_SERVE.ACTUAL_INTEREST_BILL_GROSS3 a
LEFT JOIN DB_INVESTMENTS_SYST02.INVESTMENTS_SERVE.EXPECTED_INTEREST2 e
  ON a.ARRANGEMENT_ID = e.ARRANGEMENT_ID
ORDER BY a.PAYMENT_DATE DESC, a.ARRANGEMENT_ID;
